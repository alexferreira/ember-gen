var {{modelName}} = require('{{modelPath}}');

var {{objectName}} = Ember.Route.extend({

  renderTemplate: function() {
    this.render('{{editRoute}}', {controller: '{{controller}}'});
  },

  model: function() {
    return {{modelName}}.createRecord();
  },

  exit: function() {
    var model = this.get('controller.model');
    if (!model.get('isSaving')) {
      model.deleteRecord();
    }
  }

});

module.exports = {{objectName}};

